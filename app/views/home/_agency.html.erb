<div class="row">
  <div class="col s12">
    <h1 class="header"> Travel Agencies </h1>
  </div>
  <div class="container">
    <% for i in 1..9 do %>
      <div class="col s12 m6 l4">
        <div class="card waves-effect waves-block waves-light">
          <div class="card-image">
            <%= image_tag "room1.jpg", class:"activator" %>
            <span class="card-title">Agency <%= i %></span>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>